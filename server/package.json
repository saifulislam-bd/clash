{
  "name": "clash-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "watch": "tsc -w",
    "build": "concurrently \"tsc\" \"npm run copy-files\" ",
    "server": "nodemon ./dist/index.js",
    "dev": "concurrently \"npm run watch\" \"npm run server\" \"npm run watch-views\"",
    "copy-files": "copyfiles -u 1 \"src/views/**/*\" dist/",
    "watch-views": "nodemon --watch src/views -e ejs -x \"npm run copy-files\""
  },
  "keywords": [],
  "author": "Saiful Islam",
  "license": "ISC",
  "description": "",
  "type": "module",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/socket.io": "^3.0.1",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.29.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "express-fileupload": "^1.5.1",
    "express-handlebars": "^8.0.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "prisma": "^5.22.0",
    "socket.io": "^4.8.1",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-fileupload": "^1.5.1",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.9.3",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.1.0",
    "copy-webpack-plugin": "^12.0.2",
    "copyfiles": "^2.4.1",
    "typescript": "^5.7.2"
  }
}
